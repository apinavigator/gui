{
  "name": "gui",
  "description": "API Navigator enables developers to traverse large and complex API References smoothly and efficiently through a top-notch PWA.",
  "private": false,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build && sw-builder",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview",
    "test": "echo \"Error: tests are executed with  npm run test:(e2e|integration|unit)\" && exit 1",
    "test:e2e": "vitest run --config=vitest.test-e2e.config.ts",
    "test:integration": "vitest run --config vitest.test-integration.config.ts",
    "test:unit": "vitest run --config vitest.test-unit.config.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/apinavigator/gui"
  },
  "keywords": [
    "api",
    "documentation",
    "docs",
    "web",
    "cheatsheet",
    "cheatsheets",
    "web-utils",
    "api-reference",
    "web-utilities",
    "api-references"
  ],
  "author": "Jesus Graterol",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/apinavigator/gui/issues"
  },
  "homepage": "https://github.com/apinavigator/gui#readme",
  "dependencies": {
    "@radix-ui/react-slot": "^1.0.2",
    "browser-style-normalizer": "^1.0.1",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.379.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tailwind-merge": "^2.3.0",
    "tailwindcss-animate": "^1.0.7",
    "webfont-montserrat": "^1.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.12.12",
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@typescript-eslint/eslint-plugin": "^7.11.0",
    "@typescript-eslint/parser": "^7.11.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "postcss": "^8.4.38",
    "sw-builder": "^1.0.3",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.2.2",
    "vite": "^5.2.0",
    "vitest": "^1.6.0"
  }
}
